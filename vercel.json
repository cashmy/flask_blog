{
  "version": 2,
  "builds": [
    {
      "src": "run.py", 
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" } 
    }
  ],
  "routes": [
    {
      "src": "/flaskblog/static/(.*)", 
      "dest": "/flaskblog/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "run.py" 
    }
  ]
}